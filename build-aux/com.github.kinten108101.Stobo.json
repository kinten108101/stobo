{
	"id": "com.github.kinten108101.Stobo",
	"runtime": "org.gnome.Sdk",
	"runtime-version": "46",
	"sdk": "org.gnome.Sdk",
	"command": "com.github.kinten108101.Stobo",
	"finish-args": [
		"--share=ipc",
		"--socket=fallback-x11",
		"--socket=wayland",
		"--device=dri",
		"--share=network"
	],
	"cleanup": [
		"#/include",
		"#/lib/pkgconfig",
		"/man",
		"/share/doc",
		"/share/gtk-doc",
		"/share/man",
		"#/share/pkgconfig",
		"/share/installed-tests",
		"*.la",
		"*.a"
	],
	"modules": [
		"modules/blueprint-compiler.json",
		{
			"buildsystem": "meson",
			"sources": [
				{
					"type": "dir",
					"path": ".."
				}
			]
			"name": "Stobo",
		}
	]
}
